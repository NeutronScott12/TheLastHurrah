    "scripts": {
        "docker:up": "docker-compose up -d",
        "docker:down": "docker-compose down",
        "docker:build": "docker-compose build",
        "remove:node_modules": "npx rimraf node_modules",
        "prisma:migrate": "npx prisma migrate dev",
        "prisma:generate": "npx prisma generate",
        "prisma:reset": "npx prisma migrate reset --force",
        "migrate:generate": "npx cross-env npm run prisma:migrate && npm run prisma:generate",
        "prebuild": "npx cross-env npm run test && npx rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "npx cross-env NODE_ENV=development nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "npx cross-env NODE_ENV=production node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "npx cross-env NODE_ENV=test jest",
        "test:watch": "npx cross-env NODE_ENV=test jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json"
    },
