{
    "name": "@thelasthurrah/the-last-hurrah-email",
    "version": "0.0.1",
    "description": "A starter template for nest",
    "author": "Ogneutron",
    "repository": {},
    "private": true,
    "license": "MIT",
    "scripts": {
        "remove:node_modules": "npx rimraf node_modules",
        "prisma:format": "npx prisma format",
        "migrate": "cross-env npx prisma format && npx prisma migrate dev",
        "generate": "npx prisma generate",
        "migrate:generate": "cross-env npm run migrate && npm run generate",
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "cross-env NODE_ENV=development nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "cross-env NODE_ENV=production node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "cross-env NODE_ENV=test jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@grpc/grpc-js": "^1.6.8",
        "@grpc/proto-loader": "^0.7.0",
        "@nestjs-modules/mailer": "^1.8.1",
        "@nestjs/apollo": "^10.0.19",
        "@nestjs/bull": "^0.6.0",
        "@nestjs/common": "^9.0.8",
        "@nestjs/config": "^2.2.0",
        "@nestjs/core": "^9.0.8",
        "@nestjs/graphql": "^10.0.21",
        "@nestjs/jwt": "^9.0.0",
        "@nestjs/passport": "^9.0.0",
        "@nestjs/platform-express": "^9.0.8",
        "@nestjs/throttler": "^3.0.0",
        "@prisma/client": "^4.2.0",
        "@thelasthurrah/the-last-hurrah-shared": "^0.1.17",
        "apollo-server-cache-redis": "^3.3.1",
        "apollo-server-express": "^3.10.0",
        "bull": "^4.8.5",
        "class-validator": "^0.13.2",
        "cross-env": "^7.0.3",
        "graphql": "16.5.0",
        "graphql-redis-subscriptions": "^2.5.0",
        "graphql-type-json": "^0.3.2",
        "graphql-ws": "^5.9.1",
        "helmet": "^5.1.1",
        "ioredis": "^5.2.2",
        "joi": "^17.6.0",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.5.6",
        "ts-morph": "^15.1.0"
    },
    "devDependencies": {
        "@nestjs/cli": "^9.0.0",
        "@nestjs/schematics": "^9.0.1",
        "@nestjs/testing": "^9.0.8",
        "@types/bull": "^3.15.9",
        "@types/express": "^4.17.13",
        "@types/jest": "^28.1.6",
        "@types/node": "^18.6.5",
        "@types/nodemailer": "^6.4.5",
        "@types/passport-jwt": "^3.0.6",
        "@types/passport-local": "^1.0.34",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.33.0",
        "@typescript-eslint/parser": "^5.33.0",
        "eslint": "^8.21.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^28.1.3",
        "prettier": "^2.7.1",
        "prisma": "^4.2.0",
        "source-map-support": "^0.5.21",
        "supertest": "^6.2.4",
        "ts-jest": "^28.0.7",
        "ts-loader": "^9.3.1",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.1.0",
        "typescript": "^4.7.4"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
